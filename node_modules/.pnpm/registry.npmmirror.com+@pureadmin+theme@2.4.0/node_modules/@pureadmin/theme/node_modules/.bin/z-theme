#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/taoshuaixing/Desktop/project/vue-nice-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/taoshuaixing/Desktop/project/vue-nice-admin/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+@zougt+some-loader-utils@1.4.3/node_modules/@zougt/some-loader-utils/dist/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+@zougt+some-loader-utils@1.4.3/node_modules/@zougt/some-loader-utils/dist/bin/index.js" "$@"
fi
