{
  "name": "@pureadmin/utils",
  "version": "1.7.4",
  "packageManager": "pnpm@7.0.1",
  "description": "pureadmin utils",
  "keywords": [
    "@pureadmin/utils",
    "typescript",
    "hooks",
    "vue3",
    "es6"
  ],
  "homepage": "https://github.com/xiaoxian521/pure-admin-utils/tree/main/#readme",
  "bugs": {
    "url": "https://github.com/xiaoxian521/pure-admin-utils/issues"
  },
  "license": "MIT",
  "author": "xiaoxian521",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoxian521/pure-admin-utils"
  },
  "main": "dist/cjs.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/iife.global.js",
  "jsdelivr": "dist/iife.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "dev": "tsup --watch",
    "serve": "cd playgrounds/vue3 && pnpm dev",
    "serve:react": "cd playgrounds/react && pnpm dev",
    "lint": "eslint . --fix && prettier --write  \"src/**\"",
    "typecheck": "tsc --noEmit",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "start": "esno src/index.ts",
    "pub": "pub",
    "prepare": "husky install"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "echarts": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@esbuild-kit/cjs-loader": "^2.1.0",
    "@pureadmin/release": "^0.0.2",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vueuse/core": "^9.6.0",
    "dayjs": "^1.11.5",
    "echarts": "^5.4.0",
    "eslint": "^8.15.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4",
    "vitest": "^0.24.3",
    "vue": "^3.2.41"
  }
}
