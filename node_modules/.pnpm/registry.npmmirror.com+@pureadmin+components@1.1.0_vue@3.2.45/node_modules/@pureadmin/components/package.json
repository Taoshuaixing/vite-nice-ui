{
  "name": "@pureadmin/components",
  "version": "1.1.0",
  "description": "pureadmin components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev:example": "cd ./example && yarn dev",
    "dev": "rm -rf dist && rollup -w -c ./build/rollup.config.dev.js",
    "build": "rm -rf dist && rollup -c ./build/rollup.config.prod.js",
    "tsup": "rm -rf dist && tsup src/index.ts --minify --format esm,cjs && uglifycss src/theme.css --output dist/theme.css && uglifycss src/dark.scss --output dist/dark.scss"
  },
  "keywords": [
    "@pureadmin/components",
    "ant-design-vue",
    "vue3"
  ],
  "author": "xiaoxian521",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "ant-design-vue": "^3.2.5",
    "cssnano": "^5.1.0",
    "rollup": "^2.70.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts-bundle": "^1.0.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.2",
    "uglifycss": "^0.0.29",
    "vite": "^2.9.9",
    "vue": "^3.2.36"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.2.1",
    "@ant-design/icons-vue": "^6.1.0"
  }
}
